@page "/patients"

<h3>Patients</h3>

<table class="table">
	<thead>
		<tr>
			<th>
				Id
			</th>
			<th>
				First Name
			</th>
			<th>
				Last Name
			</th>
			<th>
				Adress
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var patient in PatientsList)
		{
			<tr>
				<th>
					@patient.PatientId
				</th>
				<th>
					@patient.FirstName
				</th>
				<th>
					@patient.LastName
				</th>
				<th>
					@patient.PatientAdress
				</th>
			</tr>
		}
	</tbody>
</table>

